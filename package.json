{
  "name": "semantic-moonlight",
  "displayName": "Semantic Moonlight",
  "description": "Moonlight theme with semantic highlighting and tweaked to resemble material moonlight for JetBrains IDEs",
  "version": "0.10.5",
  "publisher": "Ricky12Awesome",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/atomiks/moonlight-vscode-theme"
  },
  "keywords": [
    "Theme",
    "Dark Theme",
    "Moonlight",
    "Night Theme",
    "Semantic Highlighting"
  ],
  "scripts": {
    "publish": "vsce publish",
    "build": "node build"
  },
  "galleryBanner": {
    "color": "#061526",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "icon.png",
  "contributes": {
    "themes": [
      {
        "label": "Semantic Moonlight",
        "uiTheme": "vs-dark",
        "path": "./themes/semantic-moonlight.json"
      }
    ]
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "vsce": "^1.71.0"
  }
